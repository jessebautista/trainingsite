import swell from 'swell-js';

// Initialize swell with your store ID and public key
swell.init('jesse-swell-internal-horizon1112', 'pk_test_e8uqpwFEn8HF2l7uvxaU0qK7FjDxxmlC');

let menus = null;

// Fetch the settings menus
const fetchMenus = async () => {
  try {
    menus = await swell.settings.menus();
    console.log('Menus:', menus);
  } catch (error) {
    console.error('Error fetching menus:', error);
  }
};

// Call the function to fetch menus
fetchMenus();
